  {
    "name": "Network Management",
    "alias": "nm",
    "type": "command",
    "args": {
      "commands": {
        "Waves": {
          "icon": "network-connect",
          "exec": "nmcli d wifi c Waves &",
          "search_string": "wifi;waves"
        },
        "Waves-AP": {
          "icon": "network-connect",
          "exec": "nmcli d wifi c Waves-AP &",
          "search_string": "wifi;wavesap"
        }
